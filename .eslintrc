{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb-base",
  "plugins": [
    "html"
  ],
  "rules": {
    "global-require": 0,
    "import/first": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "no-multi-assign": 0,
    "no-restricted-globals": 0,
    "no-underscore-dangle": 0,
    "no-bitwise": 0,
    "prefer-promise-reject-errors": 0
  }
}